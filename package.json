{
  "name": "openclaw-telegram-manager",
  "version": "1.0.0",
  "type": "module",
  "description": "Deterministic Telegram forum topic management for OpenClaw",
  "license": "MIT",
  "repository": "github:jcoulaud/openclaw-telegram-manager",
  "exports": {
    ".": "./src/index.ts",
    "./setup": "./src/setup.ts"
  },
  "bin": {
    "openclaw-telegram-manager": "./dist/setup.js"
  },
  "files": [
    "src/",
    "dist/",
    "skills/",
    "openclaw.plugin.json"
  ],
  "openclaw": {
    "extensions": [
      "./src/index.ts"
    ]
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.0"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.48",
    "json5": "^2.2.3",
    "proper-lockfile": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/proper-lockfile": "^4.1.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "lint": "tsc --noEmit",
    "audit:deps": "npm audit",
    "postversion": "git push origin main --tags"
  }
}
