{
  "name": "openclaw-telegram-manager",
  "version": "1.3.6",
  "type": "module",
  "description": "Deterministic Telegram forum topic management for OpenClaw",
  "license": "MIT",
  "repository": "github:jcoulaud/openclaw-telegram-manager",
  "exports": {
    ".": "./src/index.ts",
    "./setup": "./src/setup.ts"
  },
  "bin": {
    "openclaw-telegram-manager": "./dist/setup.js"
  },
  "files": [
    "dist/",
    "skills/",
    "openclaw.plugin.json"
  ],
  "openclaw": {
    "extensions": [
      "./dist/plugin.js"
    ]
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.5",
    "@semantic-release/npm": "^13.1.4",
    "@sinclair/typebox": "^0.34.48",
    "@types/node": "^25.2.3",
    "@types/proper-lockfile": "^4.1.4",
    "esbuild": "^0.25.0",
    "json5": "^2.2.3",
    "proper-lockfile": "^4.1.2",
    "semantic-release": "^25.0.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json && esbuild src/index.ts --bundle --platform=node --target=node18 --format=esm --outfile=dist/plugin.js",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "lint": "tsc --noEmit",
    "audit:deps": "npm audit"
  }
}
